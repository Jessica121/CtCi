
public class CtciChapter1 {
public static boolean isUnique(String a){
	int len = a.length();
	int[] map = new int[len];
	if (len >=256){
		return false;
	}
	for(int i=0; i<len;i++){
		int index = a.charAt(i);// why is int???
		System.out.println(index);
		if(map[index] == 1){
			return false;}
		else
			map[index] = 1;
	}
	return true;
}

public static void main(String args[]){
 String string = "hello";
 if(isUnique(string)){
	 System.out.println("yes");
 }else
	 System.out.println("no");
	
}

}
